{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "post"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    },
    {
      "name": "userDocument",
      "type": "cosmosDB",
      "databaseName": "BudgetStream",
      "collectionName": "Users",
      "createIfNotExists": true,
      "connectionStringSetting": "UserStore.ConnectionString",
      "direction": "out"
    },
    {
      "type": "cosmosDB",
      "name": "user",
      "databaseName": "BudgetStream",
      "collectionName": "Users",
      "connectionStringSetting": "UserStore.ConnectionString",
      "direction": "in",
      "Id": "{id}",
      "PartitionKey": "{userId}"
    }
  ],
  "scriptFile": "../dist/ExchangeToken/index.js"
}